{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abbasudo/pulse-users": "^1.0",
        "bavix/laravel-wallet": "^11.4",
        "brick/money": "^0.11.1",
        "charrafimed/global-search-modal": "^3.7",
        "chrysanthos/pulse-requests": "^1.0",
        "dedoc/scramble": "^0.12.23",
        "denniseilander/pulse-about-application": "^0.3.0",
        "denniseilander/pulse-log-files": "^0.3.0",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "google/apiclient": "^2.18",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.33",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.23",
        "laravel/telescope": "^5.10",
        "laravel/tinker": "^2.10.1",
        "matanyadaev/laravel-eloquent-spatial": "^4.5",
        "outerweb/filament-translatable-fields": "^2.1",
        "owenvoke/blade-fontawesome": "^2.9",
        "paulo-hortelan/requests-graph-pulse": "^1.2",
        "predis/predis": "^3.1",
        "propaganistas/laravel-phone": "^6.0",
        "socialiteproviders/apple": "^5.7",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/google": "^4.1",
        "spatie/browsershot": "^5.0",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-permission": "^6.20",
        "spatie/laravel-translatable": "^6.11",
        "timacdonald/pulse-validation-errors": "^1.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^2.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.25",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74,#7cff3b,#fbbf24,#34d399\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" \"php artisan reverb:start --host=0.0.0.0 --port=8080 --debug\" \"php artisan horizon\" \"php artisan pulse:check\" --names=server,queue,logs,vite,reverb,horizon,pulse"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
